# Anthropic API Key - Required for AI processing
ANTHROPIC_API_KEY=your_api_key_here

# Server port (optional, default is 3000)
PORT=3000

# Debug mode (optional, default is false)
DEBUG=false

# Maximum number of pages to crawl (optional, default is 100)
MAX_PAGES=100

# Default crawl depth (optional, default is 1)
DEFAULT_DEPTH=1

# Request timeout in milliseconds (optional, default is 30000)
REQUEST_TIMEOUT=30000
